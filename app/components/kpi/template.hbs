<div class='container'>
  <div class='ps-5 pe-5 mb-5'>
    <Kpi::Charts::MainKpiChart
      @tableOfMonths={{this.tableOfMonths}}
      @mtbf={{this.mtbf}}
      @mttr={{this.mttr}}
      @numberTimeOfBreakdowns={{this.numberTimeOfBreakdowns}}
    />
  </div>
  <div class='ps-5 pe-5 mb-5'>
    <Kpi::Charts::RequestsTypeChart
      @tableOfMonths={{this.tableOfMonths}}
      @tableOfMonthsWithRequests={{this.tableOfMonthsWithRequests}}
    />
  </div>
  <div class='ps-5 pe-5 mb-5'>
    <Kpi::Charts::RequestTypeMachineChart
      @machinesName={{this.machinesName}}
      @numberOfRequestByType={{this.numberOfRequestByType}}
    />
  </div>
</div>