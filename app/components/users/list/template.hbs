<table class='table table-striped'>
  <thead class='table-active'>
    <tr>
      <th scope='col'>ID</th>
      <th scope='col'>Username</th>
      <th scope='col'>Password</th>
      <th scope='col'>Email</th>
      <th scope='col'>Requestor</th>
      <th scope='col'>Admin</th>
    </tr>
  </thead>
  <tbody>
    {{#each @model as |user|}}
      <tr>
        <td>{{user.id}}</td>
        <td>{{user.username}}</td>
        <td>{{user.password}}</td>
        <td>{{user.email}}</td>
        <td>{{user.isRequestor}}</td>
        <td>{{user.isAdmin}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>