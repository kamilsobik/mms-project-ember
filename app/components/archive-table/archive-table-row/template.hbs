<tr class={{class-table @request.type}}>
  <td>{{@request.type}}</td>
  <td>{{@request.owner.username}}</td>
  <td>{{@request.machine}}</td>
  <td>{{@request.title}}</td>
  <td>{{@request.solution}}</td>
  <td>{{@request.downtime}}</td>
  <td>{{format-date @request.createdAt 'dd-MM-yyyy HH:mm'}}</td>
</tr>